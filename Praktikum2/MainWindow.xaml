        <Button x:Name="button4" Content="4" HorizontalAlignment="Left" Margin="137,176,0,0" VerticalAlignment="Top" Width="40" Click="button4_Click"/>
        <Button x:Name="button5" Content="5" HorizontalAlignment="Center" Margin="0,176,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.11,0.453" Width="40" Click="button5_Click"/>
